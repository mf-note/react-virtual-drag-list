{
  "parserOptions": { 
    "requireConfigFile" : false,
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "rules": {
    "no-undefined": "off",
    "no-param-reassign": "off",
    "react/no-unused-prop-types": "off"
  }
}